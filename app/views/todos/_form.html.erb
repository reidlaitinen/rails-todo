<%= form_for @todo do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, :autocomplete => :off %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description, :autocomplete => :off %>
  <br>
  <%= f.label :priority %>
  <%= f.radio_button :priority, 5 %>
  <%= label_tag(:priority, "5") %>
  <%= f.radio_button :priority, 4 %>
  <%= label_tag(:priority, "4") %>
  <%= f.radio_button :priority, 3 %>
  <%= label_tag(:priority, "3") %>
  <%= f.radio_button :priority, 2 %>
  <%= label_tag(:priority, "2") %>
  <%= f.radio_button :priority, 1 %>
  <%= label_tag(:priority, "1") %>
  <br>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
  <%= f.submit %>
<% end %>
